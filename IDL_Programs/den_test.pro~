pro den_test


  dataf = '../Data/'
  lowmlt = 12
  highmlt = 24
  lowls = 0
  highls = 8
  nummlt = 240.
  numls = 80.
  restore, dataf+'CRRES_1min_1990.save'
  restore, dataf+'CRRES_1min_1991.save'
  restore, strcompress(dataf+'CRRES_EMIC_2010.save')
                                ;Here we are creating the arrays for
                                ;the MLT, LS, and Den. for the EMIC
                                ;wave events. These arrays contain
                                ;that information for the entire
                                ;event. 
  emicmlt90 = mlt90*syearmin90;*oncrres90
  emicmlt91 = mlt91*syearmin91;*oncrres91
  emicmlat90 = mlat90*syearmin90  ;*oncrres90
  emicmlat91 = mlat91*syearmin91;*oncrres91
  emicls90 = ls90*syearmin90;oncrres90
  emicls91 = ls91*syearmin91;oncrres91
  emicden90 = den90*syearmin90;oncrres90
  emicden91 = den91*syearmin91;oncrres91  
  noindex90 = where(syearmin90 ne 1)
  noindex91 = where(syearmin91 ne 1)
  noemicyear90 = make_array(n_elements(syearmin90), value = !values.F_NAN)
  noemicyear91 = make_array(n_elements(syearmin91), value = !values.F_NAN)
  noemicyear90(noindex90) = 1.
  noemicyear91(noindex91) = 1.
  nomlt90 = mlt90*noemicyear90
  nomlt91 = mlt91*noemicyear91
  nomlat90 = mlat90*noemicyear90
  nomlat91 = mlat91*noemicyear91
  nols90 = ls90*noemicyear90
  nols91 = ls91*noemicyear91
  noden90 = den90*noemicyear90
  noden91 = den91*noemicyear91

stop
